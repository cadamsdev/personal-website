    "prismjs": "^1.21.0",
    "react": "^16.8.6",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "frontmatter-markdown-loader": "^3.6.2",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
